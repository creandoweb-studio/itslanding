//-Menu responsive
-tipo="spring"
block config
  include config.pug

mixin navigationOffCanvas(menu)
  section.Header
    section.Header-container
      h1.Logo
        a.Logo-link(href="./")=empresa
      a.Panel-button(href="#")
        button.hamburger.hamburger--spring(type="button")
          span.hamburger-box
            span.hamburger-inner
      aside.Panel
        nav.Menu
          ul.Menu-listItem
            each item in menu
              li.Menu-item
                a.Menu-link(href=`${item[1]}` target=`${item[2]}`)= item[0]
