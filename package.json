{
  "name": "frontend-components-boilerplate-by-ches",
  "version": "1.0.0",
  "description": "Boilerplate para el desarrollo de proyectos FrontEnd basado en componentes WEB con metalenguajes y automatización de tareas con script NPM",
  "main": "package.json",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "sass --watch --color ./src/scss/:./public",
    "pug": "pug -w -P -p ./src/pug/includes/ ./src/pug/pages/ -o ./public/",
    "babel": "babel -w ./src/js/ -d ./public/",
    "browserify": "browserify ./src/js/index.js -d -o ./public/script.js -t [ babelify --presets [ env ] ]",
    "watchify": "watchify ./src/js/index.js -d -o ./public/script.js -t [ babelify --presets [ env ] ] -v",
    "serve": "browser-sync start --server --files public --serveStatic public --reload-delay=300 --reload-debounce=500",
    "proxy": "browser-sync start --proxy localhost --files public --directory --startPath / --serveStatic / --reload-delay=300 --reload-debounce=500",
    "metalangs": "concurrently \"npm run sass\" \"npm run pug\" \"npm run watchify\"",
    "devserve": "concurrently \"npm run metalangs\" \"npm run serve\"",
    "devproxy": "concurrently \"npm run metalangs\" \"npm run proxy\"",
    "imagemin": "imagemin \"./src/images/**/*.+(jpg|png)\" -o ./public/images/ -p=pngquant -p=jpeg-recompress",
    "webp": "imagemin \"./public/images/**/*.jpg\" -o ./public/images/ -p=webp",
    "svgmin": "imagemin \"./src/images/**/*.svg\" -o ./public/images/ -p=svgo",
    "gifmin": "imagemin \"./src/images/**/*.gif\" -o ./public/images/ -p=gifsicle",
    "jpgresize": "magick mogrify -resize \"1024 >\" ./public/images/*.jpg",
    "pngresize": "magick mogrify -resize \"1024 >\" ./public/images/*.png",
    "statics": "node statics",
    "assets": "npm run statics && npm run imagemin && npm run webp && npm run svgmin && npm run gifmin",
    "clean": "rimraf \"./public/**\" && mkdirp ./public/images && touch ./public/index.html ./public/script.js ./public/style.css",
    "min.html": "pug -p ./src/pug/includes/ ./src/pug/pages/ -o ./public/",
    "min.js": "npm run browserify && uglifyjs ./public/script.js -c -o ./public/script.js",
    "uncss": "uncss -u uncss.json \"./public/**.html\" > ./public/style.un.css",
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers \"> 5%, ie 10\" -r ./public/style.un.css",
    "min.css": "sass --style=compressed ./public/style.un.css ./public/style.css",
    "min": "npm run min.html && npm run min.js && npm run uncss && npm run autoprefixer &&  npm run min.css && rimraf ./public/style.un.css",
    "build": "npm run min && npm run serve",
    "start": "npm run devserve"
  },
  "author": "Andrés Gómez",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.26.3",
    "browserify": "^14.5.0",
    "concurrently": "^3.0.2",
    "copy": "^0.3.2",
    "imagemin": "^5.3.1",
    "imagemin-cli": "^3.0.0",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^5.2.4",
    "imagemin-webp": "^5.0.0",
    "mkdirp": "^0.5.1",
    "postcss-cli": "^4.1.1",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.3",
    "sass": "^1.30.0",
    "uglify-js": "^3.5.1",
    "uncss": "^0.15.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "wow.js": "^1.2.2",
    "wowjs": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codigocreativoing/aterrizajeV2.1.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/codigocreativoing/aterrizajeV2.1/issues"
  },
  "homepage": "https://github.com/codigocreativoing/aterrizajeV2.1#readme"
}
